{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#6a9cff",
					"name": "Aspire-main",
					"slug": "custom-aspire-main"
				},
				{
					"color": "#96b9fe",
					"name": "Aspire-main:hover",
					"slug": "custom-aspire-main-hover"
				},
				{
					"color": "#f3f7ff",
					"name": "Aspire-main:lightest",
					"slug": "custom-aspire-main-light"
				},
				{
					"color": "#131313",
					"name": "Aspire-black",
					"slug": "custom-aspire-black"
				},
				{
					"color": "#4e5652",
					"name": "Aspire-grey",
					"slug": "custom-aspire-grey"
				},
				{
					"color": "#ffffff",
					"name": "Aspire-white",
					"slug": "custom-aspire-white"
				}
			]
		},
		"layout": {
			"contentSize": "80em",
			"wideSize": "90em"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Epilogue SemiBold",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/epilogue-semibold_normal_600.ttf"
							]
						}
					],
					"fontFamily": "Epilogue SemiBold",
					"slug": "epilogue-semibold"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Epilogue Light",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/epilogue-light_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Epilogue Light",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/epilogue-light_italic_300.ttf"
							]
						}
					],
					"fontFamily": "Epilogue Light",
					"slug": "epilogue-light"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var:preset|color|custom-aspire-white",
			"text": "var:preset|color|custom-aspire-black"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|custom-aspire-main",
					"text": "var:preset|color|custom-aspire-white"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|epilogue-semibold",
					"fontSize": "rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|epilogue-semibold",
					"fontStyle": "normal",
					"fontWeight": "600",
					"letterSpacing": "2.5px",
					"lineHeight": "1.2",
					"textTransform": "none"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|custom-aspire-main"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|custom-aspire-main:hover"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|epilogue-light"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json"
}